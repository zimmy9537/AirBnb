<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_explore">

    <fragment
        android:id="@+id/navigation_explore"
        android:name="com.zimmy.best.airbnb.view.ui.explore.ExploreFragment"
        android:label="@string/explore"
        tools:layout="@layout/fragment_explore" />

    <fragment
        android:id="@+id/navigation_wishlists"
        android:name="com.zimmy.best.airbnb.view.ui.wishlists.WishListsFragment"
        android:label="@string/wishlists"
        tools:layout="@layout/fragment_wishlists" />

    <fragment
        android:id="@+id/navigation_inbox"
        android:name="com.zimmy.best.airbnb.view.ui.inbox.InboxFragment"
        android:label="@string/inbox"
        tools:layout="@layout/fragment_inbox" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.zimmy.best.airbnb.view.ui.profile.ProfileFragment"
        android:label="@string/profile"
        tools:layout="@layout/fragment_profile"/>
</navigation>